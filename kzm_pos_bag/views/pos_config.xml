<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="aspl_pos_config_view_form" model="ir.ui.view">
        <field name="name">pos.config.form.view</field>
        <field name="model">pos.config</field>
        <field name="inherit_id" ref="kzm_pos_config.kzm_pos_config_form_view_inherit"/>
        <field name="arch" type="xml">
            <xpath expr="//div[@id='kzm_pos_config']" position='inside'>
                <div class="col-xs-12 col-md-6 o_setting_box" id="enable_bag_charges">
                    <div class="o_setting_left_pane">
                        <field name="enable_bag_charges"/>
                    </div>
                    <div class="o_setting_right_pane">
                        <label for="enable_bag_charges" string="Bag Charges"/>
                        <div class="text-muted" id="enable_bag_charges">
                            Bag Charges
                        </div>
                    </div>
                </div>
            </xpath>

        </field>
    </record>


</odoo>